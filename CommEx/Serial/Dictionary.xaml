<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CommEx.Serial">

    <local:Bool2ColorBrushConverter x:Key="Bool2ColorBrushConverter"/>
    <local:Int2ComboBoxItemConverter x:Key="Int2ComboBoxItemConverter"/>
    <local:StopBits2ComboBoxItemConverter x:Key="StopBits2ComboBoxItemConverter"/>
    <local:Parity2ComboBoxItemConverter x:Key="Parity2ComboBoxItemConverter"/>
    <local:ComboBoxItem2ParityConverter x:Key="ComboBoxItem2ParityConverter"/>
    <local:Handshake2ComboBoxItemConverter x:Key="Handshake2ComboBoxItemConverter"/>
    <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="parity">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="io:Parity"/>
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>
    <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="stop">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="io:StopBits"/>
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>
    <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="handshake">
        <ObjectDataProvider.MethodParameters>
            <x:Type TypeName="io:Handshake"/>
        </ObjectDataProvider.MethodParameters>
    </ObjectDataProvider>
</ResourceDictionary>